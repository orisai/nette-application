{
	"name": "orisai/nette-application",
	"description": "Extras for nette/application",
	"keywords": [
		"application",
		"inspector",
		"nette",
		"orisai"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-application",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"latte/latte": "^2.10.0",
		"nette/application": "^3.1.0",
		"nette/forms": "^3.0.0",
		"nette/caching": "^3.1.0",
		"orisai/nette-di": "^1.0.3",
		"tracy/tracy": "^2.8.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^2.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Application\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\Application\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
